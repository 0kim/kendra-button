{
    "projectName": "kendrabuttonhosting",
    "version": "3.0",
    "frontend": "javascript",
    "javascript": {
        "framework": "react",
        "config": {
            "SourceDir": "frontend/kendra-button-front/src",
            "DistributionDir": "frontend/kendra-button-front/src/out",
            "BuildCommand": "cd frontend/kendra-button-front && yarn build",
            "StartCommand": "cd frontend/kendra-button-front && yarn start"
        }
    },
    "providers": [
        "awscloudformation"
    ]
}